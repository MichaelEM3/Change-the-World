<div class="activities">
<h1>Activity Feed</h1>
<% @activities.each do |activity| %>
	<div class="activity">

	<% if activity.trackable %>
		<p>
			<b>
				<%= activity.trackable.title %>
			</b>
		</p>
		<p>
				<%=h truncate(activity.trackable.story, :length => 10) %>
		</p>
				<%= activity.owner.name %>
	<% else %>
		<p> This club was deleted. :( </p>
	<% end %>
<%end%>
</div>

